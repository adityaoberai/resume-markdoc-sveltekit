{
	"name": "resume",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"postinstall": "npx playwright install && npx playwright install-deps",
		"generate-pdf": "node scripts/generate-pdf.js",
		"generate-pdf:ci": "PUBLIC_PRINT_PDF=true start-server-and-test dev http://localhost:5173 generate-pdf ",
		"dev": "vite dev",
		"prebuild": "npm run generate-pdf:ci",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"lint": "prettier --check .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"start-server-and-test": "^2.0.8",
		"svelte": "^4.2.7",
		"svelte-markdoc-preprocess": "^2.1.0",
		"vite": "^5.0.3"
	},
	"type": "module"
}
